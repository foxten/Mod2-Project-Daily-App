<%= form_for @music do |f| %>

    <%= f.label :title %>
    <%= f.text_field :title %>
    
    <%= f.label :playlist %>
    <%= f.text_field :playlist %>
    
    <%= f.label :artist %>
    <%= f.text_field :artist %>

    
    <%= f.label :time_of_song %>
    <%= f.number_field :time_of_song %>

    <%= f.submit %>

    <% end %>