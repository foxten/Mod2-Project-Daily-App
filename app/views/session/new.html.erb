<p id="welcome"> WELCOME! Please sign in.</p>
<div id="body2">
    <br><% if flash[:errors] %>
        <%= flash[:errors] %><br>
    <% end %>

    <ul id="inside">
        <%= form_for :session, url: new_session_path  do |f|%>
        <%= f.label :name %>:
        <%= f.text_field :name %><br>

        <%= f.label :password %>:
        <%= f.password_field :password %><br>

        <%= f.submit "Log In" %>
    <% end %>
    </ul>
</div>
